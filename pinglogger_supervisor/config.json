{
  "name": "Ping Logger",
  "version": "0.3.1",
  "slug": "ping_logger",
  "description": "Pinguj adresy IP i publikuj sensory do Home Assistant przez MQTT",
  "startup": "application",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "map": [
    "config:rw",
    "ssl",
    "share"
  ],
  "options": {
    "targets": ["8.8.8.8", "1.1.1.1"],
    "interval": 60,
    "keep_days": 2,
    "size": 56
  },
  "schema": {
    "targets": ["str"],
    "interval": "int",
    "keep_days": "int",
    "size": "int"
  },
  "services": ["mqtt:need"]
}
