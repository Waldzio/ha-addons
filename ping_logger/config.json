{
  "name": "Ping Logger",
  "version": "0.2.3",
  "slug": "ping_logger",
  "description": "Addon: Loguje ping wybranych adres√≥w IP oraz publikuje sensory przez MQTT.",
  "startup": "application",
  "boot": "auto",
  "host_network": true,
  "options": {
    "targets": ["8.8.8.8", "1.1.1.1"],
    "interval": 1,
    "keep_days": 2,
    "mqtt_host": "localhost",
    "mqtt_port": 1883,
    "mqtt_user": "",
    "mqtt_pass": ""
  },
  "schema": {
    "targets": ["str"],
    "interval": "int",
    "keep_days": "int",
    "mqtt_host": "str",
    "mqtt_port": "int",
    "mqtt_user": "str",
    "mqtt_pass": "str"
  },
  "panel": false,
  "ingress": false,
  "arch": ["amd64", "aarch64", "armv7", "armhf"],
  "build": true
}
